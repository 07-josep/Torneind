{% block body %}{% form_theme form 'bootstrap_5_layout.html.twig' %}
    <body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                {{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}
                {{ form_widget(form) }}
                <button class="btn btn-primary">
                    <i class="fa fa-paper-plane" aria-hidden="true"></i>

                    {{ button_label|default('Enviar') }}</button>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
    </body>
{% endblock %}